TYPE "ST_RETAILER_INVOICE_HEADER" AS TABLE(
    "DISTRIBUTOR_ID" NVARCHAR(10),
    "SO_NO" NVARCHAR(10),
    "RETAILER_ID"  NVARCHAR(10),
    "INVOICE_NO" NVARCHAR(10),
    "DELIVERY_NO" NVARCHAR(10),
    "CREATION_DATE" DATE,
    "DIVISION" NVARCHAR(5),
    "REFERENCE" NVARCHAR(50),
    PRIMARY KEY("DISTRIBUTOR_ID","SO_NO","RETAILER_ID","INVOICE_NO","DELIVERY_NO")
)